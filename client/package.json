{
  "name": "react-tutorials-ui",
  "version": "0.1.0",
  "description": "Learn React more efficiently.",
  "private": true,
  "proxy": "http://localhost:5000",
  "disableHostCheck": true,
  "homepage": "https://react-tutorials.herokuapp.com",
  "bugs": "https://github.com/zenglenn42/react-tutorials/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenglenn42/react-tutorials.git"
  },
  "engines": {
    "node": "12.x",
    "yarn": "^1.22.4"
  },
  "dependencies": {
    "@testing-library/dom": "^7.21.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-router-dom": "^5.2.0",
    "react-icons": "^3.10.0",
    "typescript": "~3.7.2",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "clsx": "^1.1.1",
    "formik": "^2.1.5",
    "markdown-to-jsx": "^6.11.4",
    "recharts": "^2.0.0-beta.6",
    "short-id": "0.1.0-1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "2.0.5"
  },
  "resolutions": {
    "**/core-js": "^3.5.0",
    "**/eslint": "^6.6.0",
    "**/strip-comments": "^2.0.1",
    "**/chokidar": "^3.4.1",
    "**/source-map-resolve": "^0.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint --config ./.eslintrc.js --ext .ts --ext .tsx .",
    "lint:dryrun": "yarn run lint --fix-dry-run",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier --config ./.prettierrc.json --ignore-path ./.prettierignore  \"{src,public}/**/*.{json,js,jsx,md,ts,tsx,yml,css,html}\"",
    "format:debug": "yarn run format --loglevel debug",
    "format:check": "yarn run format --check",
    "format:ls": "yarn run format --list-different",
    "format:ci": "yarn run format:check",
    "format:write": "yarn run format --write",
    "build": "react-scripts build",
    "test:unit": "react-scripts test",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  }
}
